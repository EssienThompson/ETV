; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[animation]

warnings/check_invalid_track_paths=false

[application]

config/name="lifeblood"
run/main_scene="res://scenes/game.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DrawLine3d="*res://scripts/DrawLine3D.gd"
Events="*res://scripts/events.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
saveManager="*res://scripts/save.gd"

[dialogic]

directories/dch_directory={
"God": "res://dialog/charracters/God.dch",
"magision": "res://dialog/charracters/magision.dch"
}
directories/dtl_directory={
"magiTutCombat": "res://dialog/timelines/magision/magiTutCombat.dtl",
"magiTutElite": "res://dialog/timelines/magision/magiTutElite.dtl",
"magiTutRelic": "res://dialog/timelines/magision/magiTutRelic.dtl",
"magiTutRest": "res://dialog/timelines/magision/magiTutRest.dtl",
"magisionTutor": "res://dialog/timelines/magision/magisionTutor.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://dialog/styleRelated/style.tres", "res://dialog/styleRelated/wh9ite.tres"]
layout/default_style="res://dialog/style.tres"
variables={
"magision": {
"TutTellGoal": false
},
"player": {
"health": "",
"inDevice": ""
}
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
translation/enabled=false
translation/original_locale="en"
translation/file_mode=1
translation/translation_folder=""
translation/save_mode=0
translation/add_separator=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[global_group]

lockOn="lock on targets"
enemySpawn="where enemy's spawn"
enemy=""
interactable="interact via env not player"
naviMesh=""
persist="obj with scripts that need to be saved"
shadow=""

[importer_defaults]

texture={
"compress/channel_pack": 0,
"compress/hdr_compression": 1,
"compress/high_quality": false,
"compress/lossy_quality": 0.7,
"compress/mode": 2,
"compress/normal_map": 0,
"detect_3d/compress_to": 0,
"mipmaps/generate": false,
"mipmaps/limit": -1,
"process/fix_alpha_border": false,
"process/hdr_as_srgb": false,
"process/hdr_clamp_exposure": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": true,
"process/size_limit": 0,
"roughness/mode": 0,
"roughness/src_normal": ""
}

